    "from googleapiclient.discovery import build\n",
    "\n",
    "api_key = 'xxxx-xxxx-xxx'\n", #API key hidden
    "youtube = build('youtube', 'v3', developerKey=api_key)\n",
    "\n",
    "# Search for videos related to \"avatar movie\"\n",
    "search_response = youtube.search().list(q='avatar movie', part='snippet', type='video', maxResults=1).execute()\n",
    "video_id = search_response['items'][0]['id']['videoId']\n",
    "\n",
    "# Retrieve information about the video\n",
    "video_response = youtube.videos().list(id=video_id, part='snippet').execute()\n",
    "snippet = video_response['items'][0]['snippet']\n",
    "print(f\"Video ID: {video_id}\\n\"\n",
    "      f\"Channel ID: {snippet['channelId']}\\n\"\n",
    "      f\"Video Title: {snippet['title']}\\n\"\n",
    "      f\"Channel Title: {snippet['channelTitle']}\\n\"\n",
    "      f\"Video Description: {snippet['description']}\")"
   ]
