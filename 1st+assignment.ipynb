    "def likes_to_views_ratio(row):\n",
    "    likes = row['Likes']\n",
    "    views = row['Views']\n",
    "\n",
    "    # Avoid division by zero\n",
    "    ratio = likes / views if views != 0 else 0\n",
    "\n",
    "    return ratio\n",
    "\n",
    "def likes_views_ratio(df, top_n=10):\n",
    "    required_columns = ['Video ID', 'Title', 'Likes', 'Views', 'Comments']\n",
    "    if not all(column in df.columns for column in required_columns):\n",
    "        raise ValueError(\"The DataFrame must contain 'Video ID', 'Title', 'Likes', 'Views', and 'Comments' columns.\")\n",
    "\n",
    "    # Sort the DataFrame by 'Comments' in descending order\n",
    "    sorted_df = df.sort_values(by='Comments', ascending=False)\n",
    "\n",
    "    # Retrieve the highest comments\n",
    "    top_videos = sorted_df.head(top_n).copy()\n",
    "\n",
    "    # Calculate likes-to-views ratio \n",
    "    top_videos['Likes-to-Views Ratio'] = top_videos.apply(likes_to_views_ratio, axis=1)\n",
    "\n",
    "    # Return the DataFrame with specified columns\n",
    "    return top_videos[['Video ID', 'Title', 'Likes-to-Views Ratio']]\n",
    "\n",
    "result_df = likes_views_ratio(df)\n",
    "result_df"
   ]
